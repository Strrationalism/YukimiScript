{
    "name": "yukimiscript-syntax-highlight",
    "displayName": "YukimiScript Syntax Highlight",
    "description": "YukimiScript Syntax Highlight",
    "publisher": "seng-jik",
    "version": "0.0.2",
    "engines": {
        "vscode": "^1.69.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "yukimiscript",
            "aliases": ["Yukimi Script", "yukimiscript"],
